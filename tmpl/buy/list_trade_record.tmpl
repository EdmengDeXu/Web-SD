<table class="table table-hover">
  <thead>
    <tr>
      <th> # </th>
      <th> 时间 </th>
      <th> 收款额（元） </th>
      <th> 余额（元） </th>
      <th> 内容 </th>
    </tr>
  </thead>
  <tbody>
    {{for data}}
    <tr>
      <td> {{:#index}} </td>
      <td> {{:~formatTime(createtime)}} </td>
      <td> {{if ((moneyType==0)||(moneyType==4))}}支出{{else }}收入{{/if}}： {{:money}} </td>
      <td> 1000.00 </td>
      <td>{{:~formatMoneyType(moneyType)}}</td>
    </tr>
    {{/for}}
  </tbody>
</table>
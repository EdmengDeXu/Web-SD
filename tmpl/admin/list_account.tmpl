<table class="table table-hover table-light">
    <thead>
        <tr class="uppercase">
            <th> 序号 </th>
            <th> 账号名称 </th>
            <th> 账号等级 </th>
            <th> 是否开通白条/花呗 </th>
            <th> 收货人 </th>
            <th> 手机号 </th>
            <th> 省份 </th>
            <th> 详细地址 </th>
            <th> 我的页面 </th>
            <th> 我的账号页面 </th>
            <th> 开通白条/花呗情况 </th>
            <th> 审核状态 </th>
            <th> 审核原因 </th>
            <th> 功能 </th>
        </tr>
    </thead>
    <tbody>
      {{for data}}
        <tr>
            <td> {{:#index+1 + ~root.pageSize*~root.pageIndex}} </td>
            <td> {{:acount}}</td>
            <td> {{:acountLevel}} </td> 
            <td> {{if baitiaoStart==1}}是{{else}}否{{/if}} </td> 
            <td> {{:receiver}} </td> 
            <td> {{:receiveMobile}} </td> 
            <td> {{:receiveProvince}} </td> 
            <td class="address"> {{:receiveAddress}} </td> 
            <td> {{if mysiteImg}}<a href="{{:~root.imgPrefix}}{{:mysiteImg}}" class="fancybox"><img src="{{:~root.imgPrefix}}{{:mysiteImg}}" style="width: 150px; height: 100px;"></a>{{/if}}</td>
            <td> {{if myacountImg}}<a href="{{:~root.imgPrefix}}{{:myacountImg}}" class="fancybox"><img src="{{:~root.imgPrefix}}{{:myacountImg}}" style="width: 150px; height: 100px;"></a>{{/if}}</td>
            <td> {{if baitiaoImg}}<a href="{{:~root.imgPrefix}}{{:baitiaoImg}}" class="fancybox"><img src="{{:~root.imgPrefix}}{{:baitiaoImg}}" style="width: 150px; height: 100px;"></a>{{/if}}</td>
            
            <td class="pass{{:approve}}"> {{:approveString}} </td>
            <td> {{:approveReason}} </td>
            <td class="fn-center"> 
              {{if approve==1}}
              <a href="#basic" class="btn red btn-sm btn-outline audit-task"   data-id="{{:id}}" data-type="unpass"><i class="fa fa-edit"></i> 不通过 </a>
              <a href="#basic" class="btn blue-madison btn-sm btn-outline audit-task"   data-id="{{:id}}" data-type="pass"><i class="fa fa-edit"></i> 通过 </a>
              {{/if}}
            </td>
        </tr>
      {{/for}}
    </tbody>
  </table>